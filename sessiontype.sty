% Continuation. Usage: \st@ifcont{;}{P} --> ; P (normal)
%                      \st@ifcont{.}{P} --> .P  (nospace)
%                      \st@ifcont{;}{}  -->     (no continuation)
\newcommand{\st@ifcont}[2]{%
  \if\relax\detokenize{#2}\relax
    {}
  \else
    \ensuremath{#1\xspace#2}
  \fi
}


