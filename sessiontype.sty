% Inaction                \inact
% Restriction             \restrict{newChannelOrName}{scopeProcess}
% Recursion               \recur{declarationName}{process}
% Process Call            \vars{processName}{expressions}{sessions}
% Declare                 \declare{processName}{expressions}{sessions}{process}
